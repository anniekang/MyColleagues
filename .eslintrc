{
 "env": {
   "browser": true,
   "node": true,
   "es6": true
 },
 "extends": ["eslint:recommended", "plugin:react/recommended"],
 "parser": "babel-eslint",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "plugins": [
    "react"
  ],
  "settings": {
    "react": {
      "pragma": "React"
    }
  },
  "rules": {
    "react/prop-types": 0
  }
}
